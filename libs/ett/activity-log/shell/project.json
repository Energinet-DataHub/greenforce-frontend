{
  "name": "ett-activity-log-shell",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ett/activity-log/shell/src",
  "prefix": "ett",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/ett/activity-log/shell"],
      "options": {
        "jestConfig": "libs/ett/activity-log/shell/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/ett/activity-log/shell/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "app-ett:build:mocked"
      }
    }
  },
  "tags": ["product:ett", "domain:activity-log", "type:shell"]
}
