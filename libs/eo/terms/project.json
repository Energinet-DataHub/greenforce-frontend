    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/eo/terms/src/**/*.ts",
          "libs/eo/terms/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["product:eo", "domain:auth", "type:feature"]
}
