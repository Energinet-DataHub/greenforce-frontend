    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/eo/shared/environments/src/**/*.ts",
          "libs/eo/shared/environments/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["product:eo", "domain:shared", "type:environments"]
}
