{
  "name": "eo-shared-environments",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/eo/shared/environments/src",
  "prefix": "eo",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
