{
  "name": "e2e-eo",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/eo/e2e-eo/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/eo/e2e-eo/cypress.json",
        "tsConfig": "apps/eo/e2e-eo/tsconfig.e2e.json",
        "devServerTarget": "app-eo:serve:e2e-development",
        "headed": true
      },
      "configurations": {
        "ci": {
          "cypressConfig": "apps/eo/e2e-eo/cypress.ci.json",
          "devServerTarget": "app-eo:serve:production",
          "headed": false
        },
        "ci-development": {
          "cypressConfig": "apps/eo/e2e-eo/cypress.ci.json",
          "headed": false
        },
        "production": {
          "devServerTarget": "app-eo:serve:production"
        },
        "watch": {
          "watch": true
        },
        "watch-production": {
          "devServerTarget": "app-eo:serve:production",
          "watch": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/eo/e2e-eo/**/*.{js,ts}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["product:eo", "type:e2e"],
  "implicitDependencies": ["app-eo"]
}
