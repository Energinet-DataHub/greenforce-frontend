{
  "extends": "../../../tsconfig.base.json",
  "files": [],
  "include": ["cypress.config.ts", "src/**/*.ts", "src/**/*.js"],
  "references": [],
  "compilerOptions": {
    "baseUrl": "../../../",
    "paths": { "esbuild": ["node_modules/@badeball/cypress-cucumber-preprocessor/node_modules/esbuild"] },
    "sourceMap": false,
    "esModuleInterop": true,
    "outDir": "../../../dist/out-tsc",
    "allowJs": true,
    "types": ["@testing-library/cypress", "cypress", "node"],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "module": "nodenext",
    "moduleResolution": "nodenext"
  },
  "ts-node": {
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "nodenext"
    }
  },
  "exclude": []
}
