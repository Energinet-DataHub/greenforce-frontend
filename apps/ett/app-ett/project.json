{
  "name": "app-ett",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/ett/app-ett/src",
  "prefix": "ett",
  "tags": ["product:ett", "type:app"],
  "implicitDependencies": ["ett-shared-styles", "app-ett-landing-page", "ett-legal-feature-terms"],
  "targets": {
    "build": {
      "executor": "@nx/angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "assets": [
          {
            "glob": "favicon.ico",
            "input": "libs/ett/shared/assets/src",
            "output": ""
          },
          {
            "glob": "**/*",
            "input": "libs/ett/shared/assets/src/assets",
            "output": "assets"
          },
          {
            "glob": "**/*",
            "input": "libs/ett/legal/assets",
            "output": "assets"
          },
          {
            "glob": "**/*.json",
            "input": "libs/ett/core/globalization/assets-localization",
            "output": "assets"
          },
          {
            "input": "libs/gf/msw/util-msw/src/assets",
            "glob": "mockServiceWorker.js",
            "output": "/"
          },
          "apps/ett/app-ett/src/silent-callback.html",
          {
            "glob": "oidc-client-ts.js",
            "input": "node_modules/oidc-client-ts/dist/browser",
            "output": "/"
          }
        ],
        "allowedCommonJsDependencies": ["date-fns", "lottie-web", "dayjs"],
        "baseHref": "/",
        "index": "apps/ett/app-ett/src/index.html",
        "inlineStyleLanguage": "scss",
        "browser": "apps/ett/app-ett/src/main.ts",
        "outputPath": "dist/apps/ett/app-ett",
        "polyfills": ["zone.js"],
        "scripts": [],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/ett/shared/styles/src/lib", "libs/watt/package/core/styles"]
        },
        "styles": ["libs/ett/shared/styles/src/index.scss"],
        "tsConfig": "apps/ett/app-ett/tsconfig.app.json"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "namedChunks": true,
          "optimization": false,
          "sourceMap": true
        },
        "mocked": {
          "extractLicenses": false,
          "namedChunks": true,
          "optimization": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "libs/ett/shared/environments/src/lib/environment.ts",
              "with": "libs/ett/shared/environments/src/lib/environment.mocked.ts"
            }
          ]
        },
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1.1mb",
              "maximumError": "1.5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/ett/app-ett/src/configuration/load-ett-api-environment.ts",
              "with": "apps/ett/app-ett/src/configuration/load-ett-api-environment.prod.ts"
            },
            {
              "replace": "libs/ett/shared/environments/src/lib/environment.ts",
              "with": "libs/ett/shared/environments/src/lib/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "optimization": {
            "scripts": true,
            "styles": {
              "minify": true,
              "inlineCritical": false
            },
            "fonts": true
          }
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "buildTarget": "app-ett:build:development"
      },
      "configurations": {
        "development": {
          "hmr": true
        },
        "production": {
          "buildTarget": "app-ett:build:production"
        },
        "mocked": {
          "buildTarget": "app-ett:build:mocked",
          "host": "0.0.0.0"
        }
      },
      "defaultConfiguration": "development",
      "continuous": true
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "app-ett:build:mocked",
        "staticFilePath": "dist/apps/ett/app-ett/browser",
        "spa": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/ett/app-ett"],
      "options": {
        "jestConfig": "apps/ett/app-ett/jest.config.ts"
      }
    }
  }
}
