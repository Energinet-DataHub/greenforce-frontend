{
  "name": "e2e-ett",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ett/e2e-ett/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/ett/e2e-ett/cypress.config.ts",
        "devServerTarget": "app-ett:serve:mocked",
        "headed": true,
        "testingType": "e2e"
      },
      "configurations": {
        "ci": {
          "cypressConfig": "apps/ett/e2e-ett/cypress.config.ts",
          "devServerTarget": "app-ett:serve-static",
          "headed": false
        },
        "production": {
          "devServerTarget": "app-ett:serve:e2e"
        },
        "watch": {
          "watch": true
        },
        "watch-production": {
          "devServerTarget": "app-ett:serve:mocked",
          "watch": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["product:ett", "type:e2e"],
  "implicitDependencies": ["app-ett"]
}
